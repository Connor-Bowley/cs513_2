<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.connorbowley.cs513_2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/title"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddProductActivity"
            android:label="@string/title_activity_add_product"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.connorbowley.cs513_2.MainActivity" />
        </activity>
        <activity android:name=".AddCouponActivity" />
        <activity android:name=".GenLargestDisActivity" />
        <activity android:name=".ListProductsActivity" />
        <activity android:name=".ListCouponsActivity" />
        <activity android:name=".ViewCouponActivity" />
        <activity android:name=".DiscountUserListActivity"></activity>
    </application>

</manifest>